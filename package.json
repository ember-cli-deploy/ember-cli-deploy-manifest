{
  "name": "ember-cli-deploy-manifest",
  "version": "2.0.0",
  "description": "Ember CLI Deploy plugin to generate a manifest.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "release": "release-it",
    "test": "node tests/runner.js && ./node_modules/.bin/eslint index.js tests/**/*.js"
  },
  "repository": "https://github.com/ember-cli-deploy/ember-cli-deploy-manifest",
  "engines": {
    "node": "14.x || 16.x || 18.x || >= 20.*"
  },
  "author": "Luke Melia and ember-cli-deploy team",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "^3.28.6",
    "ember-cli-deploy-plugin": "^0.2.9",
    "eslint": "^8.42.0",
    "glob": "^10.2.6",
    "mocha": "^8.2.1",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "core-object": "^3.1.5",
    "minimatch": "^3.0.4",
    "rsvp": "^4.8.5"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "ember-addon": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    }
  }
}
